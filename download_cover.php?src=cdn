<script language="javascript">
function clearBox(object) {
  if(object.name == "searchstring" && object.value == "search") {
    object.value = '';
  }
}

function showThumb(objThumb_ID,srcThumb){
  if(document.getElementById){
    objThumb = document.getElementById(objThumb_ID);
    //objThumb.src = '/images/covers/' + srcThumb;
    objThumb.src = srcThumb;
  }
}

function hideThumb(objThumb_ID){
  if(document.getElementById){
    objThumb = document.getElementById(objThumb_ID);
    objThumb.src = '/images/indicators/indicator_snake_large.gif';
  }
}

function togglePlus(object) {
  if(document.getElementById(object).src == "http://www.thecoverproject.net/images/button_plus.gif") {
    document.getElementById(object).src = "http://www.thecoverproject.net/images/button_minus.gif";
  } else if(document.getElementById(object).src == "http://www.thecoverproject.net/images/button_minus.gif") {
    document.getElementById(object).src = "http://www.thecoverproject.net/images/button_plus.gif";
  }
}
</script>
